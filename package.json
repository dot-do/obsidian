{
  "name": "obsidian.do",
  "version": "0.1.0",
  "description": "Obsidian-compatible API for headless vault operations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "obsidian": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obsidian-do/obsidian.do.git"
  },
  "author": "obsidian.do",
  "homepage": "https://obsidian.do",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "obsidian",
    "markdown",
    "vault",
    "knowledge-graph"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.0",
    "cac": "^6.7.14",
    "hono": "^4.6.0",
    "ws": "^8.19.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  }
}
